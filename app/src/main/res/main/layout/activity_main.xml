<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.example.learn.ui.bezier.BezierLearnView
        android:id="@+id/bezierLearnView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bindRuntimeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="bindRuntimeText" />

    <TextView
        android:id="@+id/bindCompileText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bindRuntimeText"
        tools:text="bindCompileText" />

    <TextView
        android:id="@+id/bindCompileText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bindCompileText"
        tools:text="bindCompileText2" />

    <Button
        android:id="@+id/openConstraint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openConstraint"
        android:text="Open Constraint"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/openViewSwitcher"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openViewSwitcher"
        android:text="Open ViewSwitcher"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/openImageScaleType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openImageScaleType"
        android:text="Open ImageScaleType"
        app:layout_constraintBottom_toTopOf="@id/openAppbarLayout"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/openDownload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openDownload"
        android:text="Open Download"
        app:layout_constraintBottom_toTopOf="@id/openAppbarLayout2"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/loadGif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="loadGif"
        android:text="Load Gif"
        app:layout_constraintBottom_toTopOf="@id/openImageScaleType"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/openAppbarLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openAppbarLayout"
        android:text="Open AppbarLayout"
        app:layout_constraintBottom_toTopOf="@id/openWebView"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/openAppbarLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openAppbarLayout2"
        android:text="Open AppbarLayout2"
        app:layout_constraintBottom_toTopOf="@id/openWebViewSimple"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/openWebViewSimple"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openWebViewSimple"
        android:text="Open WebViewSimple"
        app:layout_constraintBottom_toTopOf="@id/openViewSwitcher"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/openWebView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openWebView"
        android:text="Open WebView"
        app:layout_constraintBottom_toTopOf="@id/openConstraint"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/iconChange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="iconChange"
        android:text="Icon Change"
        app:layout_constraintBottom_toTopOf="@id/openDownload"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/testPartition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testPartition"
        android:text="Test Partition"
        app:layout_constraintBottom_toTopOf="@id/openDownload"
        app:layout_constraintEnd_toStartOf="@id/iconChange" />

    <Button
        android:id="@+id/testNetwork"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testNetwork"
        android:text="Test Network"
        app:layout_constraintBottom_toTopOf="@id/loadGif"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/testSocial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testSocial"
        android:text="Test Social"
        app:layout_constraintBottom_toTopOf="@id/iconChange"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/testBanner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testBanner"
        android:text="Test Banner"
        app:layout_constraintBottom_toTopOf="@id/testPartition"
        app:layout_constraintEnd_toStartOf="@id/testSocial" />

    <Button
        android:id="@+id/testRefresh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testRefresh"
        android:text="Test Refresh"
        app:layout_constraintBottom_toTopOf="@id/testNetwork"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/testImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testImage"
        android:text="Test Image"
        app:layout_constraintBottom_toTopOf="@id/testSocial"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>